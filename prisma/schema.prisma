generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./prisma/dev.db"
}

model Note {
  id        Int      @id @default(autoincrement())
  title     String
  content   String  @db.Text
  tags      Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


